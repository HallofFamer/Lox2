namespace test.std
using clox.std.collection.Queue

val queue = Queue()
println("Testing class Queue...")
println("Class for queue object: ${queue.getClassName()}")
println("Queue is currently empty: ${queue.isEmpty()}")
println("")

println("Now adding elements to queue...")
queue.enqueue(2)
queue.enqueue(3.5)
queue.enqueue(true)
queue.enqueue(nil)
queue.enqueue("Hello World")
println("Initial elements in Queue: ${queue.toString()}")
println("Queue is now empty: ${queue.isEmpty()}")
println("Length of queue: ${queue.length}")
println("Converting Queue to array: ${queue.toArray()}")
println("")

println("Now iterating over queue...")
while(!queue.isEmpty()){
    println("Element: ${queue.dequeue()}")
}
println("Queue is now Empty: ${queue.isEmpty()}")
namespace test.std
using clox.std.net

val ip = net.IPAddress("127.0.0.1")
println("IP Address: ${ip}")
println("IP is v4: ${ip.isIPV4()}")
println("IP to array: ${ip.toArray()}")

val ip2 = net.IPAddress("2001:0db8:85a3:0000:0000:8a2e:0370:7334")
println("IP2 Address: ${ip2}")
println("IP2 is v6: ${ip2.isIPV6()}")
println("IP2 to array: ${ip2.toArray()}")
println("")

val domain = net.Domain("google.com")
println("Domain name: ${domain}")
println("Domain IPs: ${domain.getIPAddresses()}")

val ip3 = net.IPAddress("204.15.11.70")
println("IP3 address: ${ip3}")
println("IP3 host: ${ip3.getDomain()}")
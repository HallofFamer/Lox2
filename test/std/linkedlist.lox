namespace test.std
using clox.std.collection.LinkedList
using clox.std.util.Date

val list = LinkedList()
println("Testing class LinkedList...")
println("Class for linked list object: ${list.getClassName()}")
println("Linked list is currently empty: ${list.isEmpty()}")
println("")

list.add(2)
list.add(3.5)
list.add(true)
list.add(nil)
list.add("Hello World")
println("Initial elements in linked list: ${list.toString()}")
println("Linked list contains element 3.5: ${list.contains(3.5)}")
println("Element at index 3 of linked list: ${list.getAt(3)}")
println("")

println("Add a new element current date to the end of linked list: ")
list.add(Date.now)
println("Current elements in linked list: ${list.toString()}")
println("Add a new element 0 to the start of linked list: ")
list.addFirst(0)
println("Current elements in linked list: ${list.toString()}")
println("Add a new element false to the index 4: ")
list.addAt(4, false)
println(list.toString())
println("")

list.remove()
println("Removing first element from linked list: ${list.toString()}")
list.removeLast()
println("Removing last element from linked list: ${list.toString()}")
println("")

println("Looping through elements in linked list...")
for(val element : list){
    println("Element: ${element.toString()}")
}
println("")

println("Clearing linked list: ")
list.clear()
println("Current elements in linked list: ${list.toString()}")
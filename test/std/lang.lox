println("Testing bool...")
//val bool = Bool()
println("Class name for true: ${true.getClassName()}")
println("Class name for false: ${false.getClassName()}")
var bool = 3 > 2
println("Evaluating '3 > 2' as string: ${bool.toString()}")
println("hashcode for true is: ${true.hashCode()}")
println("hashcode for false is: ${false.hashCode()}")
println("")

println("Testing number...")
//val num = Number()
println("Class name for int 1: ${1.getClassName()}")
println("Class name for float 3.5: ${3.5.getClassName()}")
println("Convert float 2.4 to int: ${2.4.toInt()}")
println("Convert number -2.5 to string: ${(-2.5).toString()}")
println("Convert number -2 to absolute value: ${(-2).abs()}")
println("The ceil, floor and round values of 2.4 are: ${2.4.ceil()}, ${2.4.floor()}, ${2.4.round()}")
println("The greatest common divisor and least common multiple of 18 and 24 are: ${18.gcd(24)}, ${18.lcm(24)}")
println("Raise number -2 to the power of 8: ${(-2).pow(8)}")
println("Value of exp(2) is: ${2.exp()}")
println("Max and Min value of 2 and 3 is: ${2.max(3)}, ${2.min(3)}")
println("Values of sqrt(8) and cbrt(8) are: ${8.sqrt()}, ${8.cbrt()}")
println("Values of log(10), log10(10) and log2(10) are: ${10.log()}, ${10.log2()}, ${10.log10()}")
println("Values of sin(1), cos(1) and tan(1) are: ${1.sin()}, ${1.cos()}, ${1.tan()}")
println("Computing hypot(3, 4): ${3.hypot(4)}")
println("Computing factorial(5): ${5.factorial()}")

val i = 1024 * 1024 + 125 * 25
println("Integer value in base 10: ${i.toString()}")
println("Integer value in base 2: ${i.toBinary()}")
println("Integer value in base 16: ${i.toHexadecimal()}")
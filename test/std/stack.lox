namespace test.std
using clox.std.collection.Stack

val stack = Stack()
println("Testing class Stack...")
println("Class for stack object: ${stack.getClassName()}")
println("Stack is currently empty: ${stack.isEmpty()}")
println("")

println("Now adding elements to stack...")
stack.push(2)
stack.push(3.5)
stack.push(true)
stack.push(nil)
stack.push("Hello World")
println("Initial elements in stack: ${stack.toString()}")
println("Stack is now empty: ${stack.isEmpty()}")
println("Length of stack: ${stack.length}")
println("Index of element true: ${stack.search(3.5)}")
println("Converting Stack to array: ${stack.toArray()}")
println("")

println("Now iterating over stack...")
while(!stack.isEmpty()){
    println("Element: ${stack.pop()}")
}
println("Stack is now Empty: ${stack.isEmpty()}")
namespace test.features

val range = 0..4
println("Creating an instance of Range class: (${range.from}..${range.to})")
println("Length of Range: ${range.length()}")
println("Range contains value 2: ${range.contains(2)}")
println("Range element at index 3: ${range.getAt(3)}")
println("")

println("Looping through Range: ")
for(val n : range){ 
    println(n)
}
println("")

println("Looping through Range again with step 2: ")
range.step(2, fun(n){ 
    println(n)
})
println("Convert Range to Array: ${range.toArray()}")
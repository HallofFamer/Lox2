namespace test.features

class Brioche { }
println("Class Brioche: ${Brioche}")
println("Instance of class Brioche: ${Brioche()}")
println(Brioche())

class Pair { 

    equals(pair) { 
        return (this.first == pair.first) and (this.second == pair.second)
    }    
}

val pair = Pair()
pair.first = 1
pair.second = 2
println("Adding the pairs: ${pair.first + pair.second}")
println("")

val object = Object()
println("Printing object: ${object}")
println("Object is equal to 'Hello World': ${object.equals("Hello World")}")
println("Object has field length: ${object.hasField("length")}")
println("Object's class: ${object.getClass()}")
println("Object is instance of class Object: ${object.instanceOf(Object)}")
println("Object is member of class Object: ${object.memberOf(Object)}")
println("Converting object to string: ${object.toString()}")
println("")

val pair2 = pair.clone()
println("Clone of pair and adding the fields: ${pair2.first + pair2.second}")
println("Compare pairs with equality operator: ${pair == pair2}")
println("Pair is equal to its clone: ${pair.equals(pair2)}")
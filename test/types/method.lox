namespace test.types

class Calculator {
    
    Int sumInt(Int a, Int b) {
        return a + b
    }

    class Number sumNumber(Number a, Number b) { 
        return a + b
    }

    void printSum(Int a, Int b) {
        println(a + b)
    }
}

val calc = Calculator()
println(calc.sumInt(1, 2))
println(Calculator.sumNumber(1.5, 2.5))
calc.printSum(2, 4)
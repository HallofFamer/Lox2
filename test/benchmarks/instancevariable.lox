namespace test.benchmarks

class Zoo {
  __init__() {
    this.aardvark = 1
    this.baboon   = 1
    this.cat      = 1
    this.donkey   = 1
    this.elephant = 1
    this.fox      = 1
  }
  ant()    { return this.aardvark }
  banana() { return this.baboon }
  tuna()   { return this.cat }
  hay()    { return this.donkey }
  grass()  { return this.elephant }
  mouse()  { return this.fox }
  
  benchmark(){ 
    var sum = 0
	val start = clock()
	while (sum < 10000000) { 
      sum = sum + this.aardvark + this.baboon + this.cat + this.donkey + this.elephant + this.fox
    }
	
	println("Time taken for property access: ${clock() - start} seconds")
    println("Sum: ${sum}")
  }
}

val zoo = Zoo()
zoo.benchmark()